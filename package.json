{
  "name": "node-red-contrib-connio",
  "version": "0.1.1",
  "description": "Node-RED nodes for Connio",
  "repository": "git@github.com:connio/node-red-contrib-connio.git",
  "author": "Connio Inc.",
  "license": "MIT",
  "keywords": [
    "connio",
    "node-red"
  ],
  "contributors": [
    {
      "name": "Ivan Gerasimov"
    }
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development concurrently --raw -n nodemon,browser-sync --kill-others-on-fail 'nodemon -e js,html,json --watch nodes --exec node-red -u .node-red' 'npx browser-sync start -c .browsersyncrc.js'"
  },
  "node-red": {
    "nodes": {
      "connio-config": "nodes/config/config.js",
      "connio-credentials": "nodes/credentials/credentials.js",
      "connio-mqtt": "nodes/mqtt/mqtt.js"
    }
  },
  "dependencies": {
    "axios": "0.19.0",
    "mqtt": "3.0.0"
  },
  "devDependencies": {
    "browser-sync": "2.26.7",
    "concurrently": "5.0.0",
    "cross-env": "6.0.3",
    "node-red": "1.0.3",
    "nodemon": "2.0.1"
  }
}
