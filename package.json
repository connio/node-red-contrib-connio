{
  "name": "node-red-contrib-connio",
  "version": "0.1.0-alpha.2",
  "description": "Node-RED nodes for Connio",
  "repository": "git@github.com:connio/node-red-contrib-connio.git",
  "author": "Connio Inc.",
  "license": "MIT",
  "keywords": [
    "connio",
    "node-red"
  ],
  "contributors": [
    {
      "name": "Ivan Gerasimov"
    }
  ],
  "scripts": {
    "start": "concurrently --kill-others-on-fail 'nodemon -e js,html --watch nodes --exec node-red' 'npx browser-sync start -c .browsersyncrc.js' "
  },
  "node-red": {
    "nodes": {
      "connio-config": "nodes/config/config.js",
      "connio-credentials": "nodes/credentials/credentials.js",
      "connio": "nodes/mqtt/mqtt.js"
    }
  },
  "dependencies": {
    "dotenv": "8.1.0",
    "mqtt": "3.0.0",
    "request": "2.88.0",
    "request-promise-native": "1.0.7"
  },
  "devDependencies": {
    "browser-sync": "2.26.7",
    "concurrently": "4.1.2",
    "node-red": "0.20.8",
    "nodemon": "1.19.2"
  }
}
